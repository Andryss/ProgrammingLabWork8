<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ChoiceBox?>


<ScrollPane fx:id="movieKeyParamPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.MovieKeyParamPaneController">
    <content>
        <BorderPane>
            <bottom>
                <HBox BorderPane.alignment="CENTER">
                    <children>
                        <Button mnemonicParsing="false" onMouseClicked="#returnToTheMainPaneMouseClicked" text="Return" />
                        <Button fx:id="movieKeyParamConfirmButton" mnemonicParsing="false" onMouseClicked="#movieKeyConfirmMouseClicked" text="Confirm" />
                        <Label fx:id="movieKeyParamConfirmErrLabel" textFill="#b00000" />
                    </children>
                </HBox>
            </bottom>
            <top>
                <HBox BorderPane.alignment="CENTER" />
            </top>
            <center>
                <GridPane BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" />
                        <ColumnConstraints hgrow="SOMETIMES" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label fx:id="movieKeyLabel" minWidth="200.0" text="Movie key:" />
                        <HBox GridPane.columnIndex="1">
                            <children>
                                <TextField fx:id="movieKeyTextField" promptText="e.g. 5555" />
                                <Button fx:id="movieKeyCheckButton" mnemonicParsing="false" onMouseClicked="#checkMovieKeyMouseClicked" text="Check" />
                            </children>
                        </HBox>
                        <Label fx:id="movieKeyErrLabel" textFill="#b00000" GridPane.columnIndex="2" />
                        <Label minWidth="200.0" text="Movie owner:" GridPane.rowIndex="1" />
                        <TextField fx:id="movieOwnerTextField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label minWidth="200.0" text="Movie id:" GridPane.rowIndex="2" />
                        <TextField editable="false" text="&lt;no one knows&gt;..." GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label minWidth="200.0" text="Movie name:" GridPane.rowIndex="3" />
                        <TextField fx:id="movieNameTextField" promptText="e.g. Terminator" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Label fx:id="movieNameErrLabel" textFill="#b00000" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                        <Label minWidth="200.0" text="Coordinates X:" GridPane.rowIndex="4" />
                        <TextField fx:id="coordinatesXTextField" promptText="e.g. 3.1415926" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <Label fx:id="coordinatesXErrLabel" textFill="#b00000" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                        <Label minWidth="200.0" text="Coordinates Y:" GridPane.rowIndex="5" />
                        <TextField fx:id="coordinatesYTextField" promptText="e.g. 1 or -14.9" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <Label fx:id="coordinatesYErrLabel" textFill="#b00000" GridPane.columnIndex="2" GridPane.rowIndex="5" />
                        <Label minWidth="200.0" text="Movie creation date:" GridPane.rowIndex="6" />
                        <TextField fx:id="movieCreationDateTextField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                        <Label minWidth="200.0" text="Movie oscars count:" GridPane.rowIndex="7" />
                        <TextField fx:id="movieOscarsCountTextField" promptText="e.g. 9223372036854775807" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                        <Label fx:id="movieOscarsCountErrLabel" textFill="#b00000" GridPane.columnIndex="2" GridPane.rowIndex="7" />
                        <Label minWidth="200.0" text="Movie length:" GridPane.rowIndex="8" />
                        <TextField fx:id="movieLengthTextField" promptText="e.g. 6420" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                        <Label fx:id="movieLengthErrLabel" textFill="#b00000" GridPane.columnIndex="2" GridPane.rowIndex="8" />
                        <Label minWidth="200.0" text="Movie genre:" GridPane.rowIndex="9" />
                        <ChoiceBox fx:id="movieGenreChoiceBox" maxWidth="1.7976931348623157E308" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="9" />
                        <Label fx:id="movieGenreErrLabel" textFill="#b00000" GridPane.columnIndex="2" GridPane.rowIndex="9" />
                        <Label minWidth="200.0" text="Movie mpaa rating:" GridPane.rowIndex="10" />
                        <ChoiceBox fx:id="movieMpaaRatingChoiceBox" maxWidth="1.7976931348623157E308" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="10" />
                        <Label minWidth="200.0" text="Screenwriter name:" GridPane.rowIndex="11" />
                        <TextField fx:id="screenwriterNameTextField" promptText="e.g. James Francis Cameron" GridPane.columnIndex="1" GridPane.rowIndex="11" />
                        <Label fx:id="screenwriterNameErrLabel" textFill="#b00000" GridPane.columnIndex="2" GridPane.rowIndex="11" />
                        <Label minWidth="200.0" text="Screenwriter birthday:" GridPane.rowIndex="12" />
                        <TextField fx:id="screenwriterBirthdayTextField" promptText="e.g. 16.08.1954" GridPane.columnIndex="1" GridPane.rowIndex="12" />
                        <Label fx:id="screenwriterBirthdayErrLabel" textFill="#b00000" GridPane.columnIndex="2" GridPane.rowIndex="12" />
                        <Label minWidth="200.0" text="Screenwriter hair color:" GridPane.rowIndex="13" />
                        <ChoiceBox fx:id="screenwriterHairColorChoiceBox" maxWidth="1.7976931348623157E308" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="13" />
                    </children>
                </GridPane>
            </center>
        </BorderPane>
    </content>
</ScrollPane>